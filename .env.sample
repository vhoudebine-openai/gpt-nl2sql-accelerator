SQL_CONNECTOR_CONFIG='{"type": "DATABRICKS", "host": "https://adb-<workspace>.azuredatabricks.net", "token": "dapiXXXXXXXXXXXXXXXXXXXX", "warehouse_id": "1234-567890-abcd123", "catalog": "main", "schema": "default"}'
